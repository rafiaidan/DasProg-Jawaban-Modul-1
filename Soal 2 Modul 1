#include<stdio.h>

int main(){
    int angka_masuk, satuan, puluhan, ratusan;
    //identify per angka
    scanf("%d", &angka_masuk);
    satuan = angka_masuk % 10;
    puluhan = angka_masuk % 100 / 10;
    ratusan = angka_masuk / 100;

    //identify ratusan
    if (ratusan == 1){
        printf("Seratus");
    }
    
        switch(ratusan){
            case 2:
            printf("Dua");
            break;

            case 3:
            printf("Tiga");
            break;

            case 4:
            printf("Empat");
            break;

            case 5:
            printf("Lima");
            break;

            case 6:
            printf("Enam");
            break;
            
            case 7:
            printf("Tujuh");
            break;

            case 8:
            printf("Delapan");
            break;

            case 9:
            printf("Sembilan");
            break;
        }
        printf(" Ratus ");
    

    //identify puluhan 10 and belasan
    if(puluhan == 1 && satuan == 0 ){
        printf("sepuluh");
    }
    else if(puluhan == 1 && satuan == 1){
        printf("sebelas");
    }
    else if(puluhan == 1 && satuan > 1){
        switch(satuan){
            case 2:
            printf("Dua");
            break;

            case 3:
            printf("Tiga");
            break;

            case 4:
            printf("Empat");
            break;

            case 5:
            printf("Lima");
            break;

            case 6:
            printf("Enam");
            break;
            
            case 7:
            printf("Tujuh");
            break;

            case 8:
            printf("Delapan");
            break;

            case 9:
            printf("Sembilan");
            break;
        }
        printf("belas");
    }

    //identify puluhan
    if(puluhan > 1 ){
        switch(puluhan){
        case 2:
            printf("Dua");
            break;

            case 3:
            printf("Tiga");
            break;

            case 4:
            printf("Empat");
            break;

            case 5:
            printf("Lima");
            break;

            case 6:
            printf("Enam");
            break;
            
            case 7:
            printf("Tujuh");
            break;

            case 8:
            printf("Delapan");
            break;

            case 9:
            printf("Sembilan");
            break;
        }
        printf(" Puluh ");
    }
  //identify satuan
  if(satuan > 0){
    switch (satuan){
            case 2:
            printf("Dua");
            break;

            case 3:
            printf("Tiga");
            break;

            case 4:
            printf("Empat");
            break;

            case 5:
            printf("Lima");
            break;

            case 6:
            printf("Enam");
            break;
            
            case 7:
            printf("Tujuh");
            break;

            case 8:
            printf("Delapan");
            break;

            case 9:
            printf("Sembilan");
            break;
    }
  }

    
}
